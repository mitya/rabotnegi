<% vacancy = object %>

<tr id='vacancy_<%= vacancy.id %>' class='<%= cycle '', 'alt' %>'>
  <td class='title'><%= vacancy.title %></td>
  <td class='employer'><%= vacancy.employer_name %></td>
  <td class='actions'>
    <button class='edit'>Edit</button>
    <button class='delete'>Delete</button>
  </td>
</tr>
